{
  "manifest_version": 2,

  "name": "Beacon Hud",
  "description": "Shows job counts from beacon",
  "version": "2.0",
  "options_page": "options.html",
  "background": {
    "scripts": ["bg.js"]
  },
  "icons": {
    "128": "icon.png"
    },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://beacon.ses.nsw.gov.au/"
  ]
}
