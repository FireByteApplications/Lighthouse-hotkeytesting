{
  "manifest_version": 2,

  "name": "Beacon Enhancement Suite (BES)",
  "description": "Provides multiple enhanements to the beacon website. accessed via the BES menu in beacon",
  "version": "3.0",
  "options_page": "options.html",
  "icons": {
    "128": "128.png",
    "64":"64.png"
    },
  "permissions": [
    "tabs",
    "storage",
    "https://beacon.ses.nsw.gov.au/"
  ],
  "content_scripts": [
    {
      "matches": ["https://beacon.ses.nsw.gov.au/*"],
      "js": ["beacon.js"]
    }
  ],
  "web_accessible_resources": ["*.html"]

}
